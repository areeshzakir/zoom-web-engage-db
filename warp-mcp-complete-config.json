{
  "mcpServers": {
    "playwright": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "mcp/playwright"],
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "youtube-transcript": {
      "command": "docker", 
      "args": ["run", "-i", "--rm", "mcp/youtube-transcript"],
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "render": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "mcp/render"],
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "fetch": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "mcp/fetch"],
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "puppeteer": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "mcp/puppeteer"],
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "context7": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "-e", "CONTEXT7_API_KEY", "mcp/context7"],
      "env": {
        "CONTEXT7_API_KEY": "<YOUR_CONTEXT7_API_KEY>"
      },
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "notion": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "-e", "NOTION_API_KEY", "mcp/notion"],
      "env": {
        "NOTION_API_KEY": "<YOUR_NOTION_API_KEY>"
      },
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "github": {
      "command": "docker",
      "args": ["run", "-i", "--rm", "-e", "GITHUB_PERSONAL_ACCESS_TOKEN", "ghcr.io/github/github-mcp-server"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "<YOUR_GITHUB_TOKEN>"
      },
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/Users/classplus/Ingestion Project test"],
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "linear": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.linear.app/sse"],
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "sentry": {
      "command": "npx",
      "args": ["-y", "mcp-remote@latest", "https://mcp.sentry.dev/mcp"],
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "slack": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-slack"],
      "env": {
        "SLACK_BOT_TOKEN": "<YOUR_SLACK_BOT_TOKEN>",
        "SLACK_APP_TOKEN": "<YOUR_SLACK_APP_TOKEN>",
        "SLACK_TEAM_ID": "<YOUR_SLACK_TEAM_ID>",
        "SLACK_CHANNEL_IDS": "<YOUR_CHANNEL_IDS>",
        "MCP_MODE": "stdio"
      },
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "atlassian": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.atlassian.com/v1/sse"],
      "working_directory": "/Users/classplus/Ingestion Project test"
    },
    "grafana": {
      "command": "docker",
      "args": ["run", "--rm", "-i", "-e", "GRAFANA_URL", "-e", "GRAFANA_API_KEY", "mcp/grafana", "-t", "stdio", "-debug"],
      "env": {
        "GRAFANA_URL": "http://localhost:3000",
        "GRAFANA_API_KEY": "<YOUR_GRAFANA_API_KEY>"
      },
      "working_directory": "/Users/classplus/Ingestion Project test"
    }
  }
}